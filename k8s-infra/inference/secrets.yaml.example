# Example secret file - copy to secrets.yaml and fill in real values
apiVersion: v1
kind: Secret
metadata:
  name: aws-credentials
  namespace: inference
type: Opaque
stringData:
  AWS_ACCESS_KEY_ID: "YOUR_ACCESS_KEY_HERE"
  AWS_SECRET_ACCESS_KEY: "YOUR_SECRET_KEY_HERE"
---
apiVersion: v1
kind: Secret
metadata:
  name: mlflow-config
  namespace: inference
type: Opaque
stringData:
  MLFLOW_TRACKING_URI: "http://mlflow-server.mlflow.svc.cluster.local:5000"
